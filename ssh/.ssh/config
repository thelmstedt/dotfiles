ServerAliveInterval 300

ControlMaster auto
ControlPath /tmp/ssh_mux_%h_%p_%r
ControlPersist 1h

Host *
  SendEnv LANG LC_*
  ForwardAgent yes

Host 192.168.2.100
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  PasswordAuthentication no



